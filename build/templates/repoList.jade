table.u-full-width
	thead
		tr
			th Rebository
			th Owner
			th Issues
			th Forks
			th Size
	tbody
	each item in items    
		tr
			td
				div.six.columns
					a(href="#", onClick='repoView("' + item.full_name + '")')
						= item.full_name
			td=item.owner.login
			td=item.open_issues
			td=item.forks
			td=item.size
			
		


